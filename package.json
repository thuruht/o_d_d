{
  "name": "overland-destinations-database",
  "version": "3.0.0",
  "private": true,
  "scripts": {
    "build": "mkdir -p dist && esbuild src/index.ts --bundle --format=esm --outfile=dist/worker.js --external:nanoid --external:drizzle-orm --external:@hono/zod-validator --external:uuid",
    "dev": "wrangler dev --local",
    "deploy": "npm run build && wrangler deploy",
    "start": "wrangler dev"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.592.0",
    "@aws-sdk/s3-request-presigner": "^3.592.0",
    "@hono/zod-validator": "^0.7.0",
    "@noble/hashes": "^1.4.0",
    "@types/nanoid": "^2.1.0",
    "drizzle-orm": "^0.44.2",
    "hono": "^4.7.11",
    "nanoid": "^5.1.5",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240605.0",
    "@types/uuid": "^9.0.8",
    "esbuild": "^0.25.5",
    "wrangler": "^4.19.2"
  }
}
